{
  "name": "@fyutrex/clickorm",
  "version": "1.0.7",
  "description": "Production-ready, type-safe ORM for ClickHouse with comprehensive TypeScript support, relations, lifecycle hooks, auto-migration, and MongoDB-style query compatibility. Designed to rival Sequelize in robustness and TypeORM in type safety.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "dev": "tsup src/index.ts --format esm --watch",
    "test": "jest --config jest.config.cjs",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "test:watch": "jest --config jest.config.cjs --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"examples/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepublishOnly": "npm run typecheck && npm run lint && npm run test && npm run build",
    "postinstall": "husky"
  },
  "keywords": [
    "clickhouse",
    "orm",
    "typescript",
    "type-safe",
    "database",
    "query-builder",
    "sql",
    "relations",
    "associations",
    "migration",
    "hooks",
    "lifecycle",
    "sequelize-like",
    "typeorm-like",
    "drizzle-like",
    "mongodb-compatibility",
    "analytics",
    "olap",
    "big-data",
    "time-series",
    "data-warehouse",
    "column-store",
    "batch-processing",
    "streaming",
    "aggregation",
    "schema-validation",
    "type-inference",
    "fluent-api",
    "production-ready",
    "enterprise",
    "node",
    "javascript",
    "ts",
    "db",
    "client",
    "driver",
    "connection-pooling",
    "auto-migration",
    "lazy-loading",
    "eager-loading",
    "sql-injection-prevention"
  ],
  "author": {
    "name": "Fyutrex Team",
    "url": "https://github.com/fyutrex"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fyutrex/clickorm.git"
  },
  "bugs": {
    "url": "https://github.com/fyutrex/clickorm/issues"
  },
  "homepage": "https://clickorm.fyutrex.com/",
  "engines": {
    "node": ">=20.0.0"
  },
  "peerDependencies": {
    "@clickhouse/client": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/fyutrex"
  }
}
